{
    "name": "PS AI Runbook Generator Pipeline",
    "email_notifications": {},
    "timeout_seconds": 3600,
    "max_concurrent_runs": 1,
    "tasks": [
        {
            "task_key": "ingestion",
            "notebook_task": {
                "notebook_path": "/Users/user@databricks.com/PS_AI/ingestion_notebook",
                "source": "WORKSPACE"
            },
            "job_cluster_key": "Shared_Cluster",
            "timeout_seconds": 600
        },
        {
            "task_key": "nlp_processing",
            "depends_on": [
                {
                    "task_key": "ingestion"
                }
            ],
            "notebook_task": {
                "notebook_path": "/Users/user@databricks.com/PS_AI/nlp_notebook",
                "source": "WORKSPACE"
            },
            "job_cluster_key": "Shared_Cluster",
            "timeout_seconds": 600
        },
        {
            "task_key": "embeddings",
            "depends_on": [
                {
                    "task_key": "nlp_processing"
                }
            ],
            "notebook_task": {
                "notebook_path": "/Users/user@databricks.com/PS_AI/embeddings_notebook",
                "source": "WORKSPACE"
            },
            "job_cluster_key": "Shared_Cluster",
            "timeout_seconds": 600
        },
        {
            "task_key": "generation",
            "depends_on": [
                {
                    "task_key": "embeddings"
                }
            ],
            "notebook_task": {
                "notebook_path": "/Users/user@databricks.com/PS_AI/runbook_generator_notebook",
                "source": "WORKSPACE"
            },
            "job_cluster_key": "Shared_Cluster",
            "timeout_seconds": 900
        }
    ],
    "job_clusters": [
        {
            "job_cluster_key": "Shared_Cluster",
            "new_cluster": {
                "spark_version": "13.3.x-scala2.12",
                "node_type_id": "i3.xlarge",
                "num_workers": 1
            }
        }
    ]
}