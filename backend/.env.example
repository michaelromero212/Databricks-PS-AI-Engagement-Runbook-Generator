# Databricks Connection Settings
# You can find your host URL in your browser address bar (e.g., https://community.cloud.databricks.com)
DATABRICKS_HOST=https://community.cloud.databricks.com

# Generate this in User Settings -> Developer -> Access Tokens
DATABRICKS_TOKEN=dapi_your_personal_access_token_here

# The ID of the job you created in the Databricks Workflows UI
DATABRICKS_JOB_ID=123456789
